{
  "name": "release-it-lerna-changelog",
  "version": "4.0.1",
  "description": "release-it plugin for lerna-changelog ",
  "keywords": [
    "release",
    "release-it",
    "release-it-plugin",
    "plugin",
    "changelog",
    "lerna"
  ],
  "repository": "https://github.com/rwjblue/release-it-lerna-changelog",
  "license": "MIT",
  "author": "Robert Jackson <me@rwjblue.com>",
  "main": "index.js",
  "files": [
    "index.js",
    "cjs-wrapper.cjs"
  ],
  "scripts": {
    "lint:js": "eslint .",
    "test": "ava"
  },
  "ava": {
    "serial": true
  },
  "type": "module",
  "dependencies": {
    "execa": "^5.1.1",
    "lerna-changelog": "^2.2.0",
    "mdast-util-from-markdown": "^1.0.4",
    "tmp": "^0.2.1",
    "validate-peer-dependencies": "^2.0.0",
    "which": "^2.0.2"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.1",
    "release-it": "^14.11.6",
    "sinon": "^11.1.2"
  },
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./cjs-wrapper.cjs"
    }
  },
  "peerDependencies": {
    "release-it": "^14.0.0"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >= 16"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "release-it": {
    "plugins": {
      "./cjs-wrapper.cjs": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true
    }
  },
  "volta": {
    "node": "14.18.1",
    "npm": "8.0.0"
  }
}
